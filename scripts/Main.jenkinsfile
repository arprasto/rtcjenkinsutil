pipeline
{
  agent any
  stages
  {
    stage('Load Configuration')
    {
    steps
      {
      script
        {
        def workspace = env.WORKSPACE
        def CONFIG_PATH= "${workspace}/rtcjenkinsutil/config/config.properties"
        def setEnvVariables = load "${workspace}/rtcjenkinsutil/libs/setEnvironment.groovy"
        echo "${CONFIG_PATH}"
        setEnvVariables.setEnvVars(CONFIG_PATH)
        }
      }
    }
  }
}
